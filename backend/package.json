{
  "name": "fdd-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --pretty --poll src/ingest.ts",
    "dev:with-mock": "cross-env USE_ML_CLIENT_MOCK=true ts-node-dev --respawn --transpile-only --pretty --poll src/ingest.ts",
    "dev:compiled": "npm run build && node dist/ingest.js",
    "start": "node dist/ingest.js",
    "build": "tsc -p .",
    "test": "jest --runInBand",
    "smoke": "tsc && node dist/ingest.js"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "google-auth-library": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.2",
    "cross-env": "^10.1.0",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
